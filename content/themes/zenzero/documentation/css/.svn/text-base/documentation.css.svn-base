/* CSS Document */
*{margin:0;padding:0}
html,body{font-family: 'PT Sans', sans-serif;margin:0;padding:0;height:100%;color:#808080;font-size:14px;}
img {border:0;padding:0;border:0}
a:focus {outline:none}
a{text-decoration:underline;color:#808080;}

/*HEADER*/
header{max-width:800px;margin:0 auto;padding:0 15px;overflow:hidden;}


.btn-top{position:fixed;bottom:10px;right:25px;}

h1{font-size: 25px;font-weight: bold;line-height: 23px;padding:50px 0 5px 0;color:#3F3F3F}
h2{font-size: 20px;font-weight: bold;line-height: 21px;padding-top: 40px;color:#606060}
h3{font-size: 16px;line-height: 19px;font-weight: bold;padding: 50px 0 10px 0;color:#707070}
h4{font-size: 13px;font-weight: bold;padding-top: 19px;margin-bottom: 0;color:#707070}

h1+h3, h1+h2{padding-top:5px}
h3+h4{padding-top:0px}
/*CONTENT*/
#content{max-width:800px;margin:0 auto 50px auto;padding:0 15px 50px 15px;overflow:hidden}
.line{border-bottom: 1px dotted #DFDFDF;padding-bottom:40px;width:100%}
p{padding-top:8px}
h3 + p {padding-top: 0;}h4 + p {padding-top: 0;}
ul.normal_list {
list-style: disc;
display: block;
padding-top: 11px;}
ul.normal_list li {
list-style: disc;
float: none;
line-height: 19px;
margin: 5px 25px;}


nav ul{list-style-type:none;margin-top:15px}
nav ul ul{margin-top:0px}
nav ul li{margin-top:5px;}
nav ul li a{margin-top:5px;color:#606060}
nav ul li ul li a{color:#808080} 
nav ul li ul{margin-left:20px;}

ul.navmenu{margin-top:10px}
.toggle {cursor:pointer;color:#999;height:23px;position:absolute;left:0}
/*Menu Setup*/

ul.navmenu li {margin:0;list-style:none;display:block;position:relative;padding:8px 20px;font-weight:bold;color:#3F3F3F}
 /*Links*/
ul.navmenu a, #menu ul.navmenu a:visited {text-decoration:none;}
ul.navmenu ul.submenu {padding:0;}
ul.navmenu ul.submenu li{display:block;padding-left:0;font-weight:normal;color:#808080}


ul.doc-list,ul.doc-list ol{margin-left: 30px; padding-left: 10px;}
ul.doc-list{margin-top: 10px;margin-bottom: 10px;}


.C-green{color:#77AA6B}.C-grey{color:#CFCFCF}
.font-code{font-family:"Courier New", Courier, monospace;color:#79C021}
.font-link{color:#E27CCE}
.font-java{font-family:"Courier New", Courier, monospace;color:#5590CF}
.font-css-code{color:#EC9E28;}
.font-css{color:#EC9E28;background-color:#F5F5F5;padding:6px 15px;width:100%;display:inline-block}


pre.prettyprint {overflow: scroll;}

.codeLine{background-color: rgb(150, 150, 150);color: #fff;font-weight: bold;padding: 0 2px;}